public class assesment1
{
  public static int removeduplicates(int a[], int n)
{
if(n == 0 || n == 1)
{
   return n;
}
int i=0;
for (int i=0;i<n-1;i++)
{
if(a[i] = a[i+1])
{
a[j++] = a[i];
}
}
a[j++] = a[n-1];
return 1;
}

public static void main(String[] args)
{
  int a[] = {1,2,3,3,4,5,6,7,7,8,9,9};
  int n = a.length;
int i=0;
j= removeduplicates(a,n);
for (int i=0;i<j;i++)
   System.out.println(a[i] + " ");
}
}
